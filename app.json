{
  "expo": {
    "name": "Kura",
    "slug": "Kura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kura-logo",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/kura-logo",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/kura-logo",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.CAMERA"],
      "package": "com.isloot.Kura"
    },
    "web": {
      "favicon": "./assets/kura-logo"
    },
    "extra": {
      "eas": {
        "projectId": "70033aa2-10dd-440b-a803-9cecaedf7422"
      }
    },
    "plugins": [
      "expo-font",
      [
        "react-native-vision-camera",
        {
          "enableCodeScanner": true
        }
      ]
    ]
  }
}
